{
  "type": "module",
  "name": "vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "cd backend && npm run dev",
    "inst:frontend": "npm install",
    "inst:backend": "cd backend && npm install",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "format": "prettier --write .",
    "install-dev": "concurrently \"npm run inst:frontend\" \"npm run inst:backend\"",
    "build": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "e2e:open": "cypress open",
    "e2e:run": "cypress run",
    "check-all": "npm run build && npm run lint && npm run format",
    "sonar": "npx sonar-scanner -Dproject.settings=sonar-project-local.properties",
    "sonar:coverage": "npm run test:cov && npx sonar-scanner -Dproject.settings=sonar-project-local.properties",
    "deps:madge": "node scripts/generate-madge.cjs"
  },
  "dependencies": {
    "@heroui/alert": "2.2.23",
    "@heroui/avatar": "^2.2.20",
    "@heroui/button": "2.2.24",
    "@heroui/card": "^2.2.23",
    "@heroui/chip": "^2.2.20",
    "@heroui/code": "2.2.18",
    "@heroui/divider": "^2.2.17",
    "@heroui/dropdown": "2.3.24",
    "@heroui/input": "2.4.24",
    "@heroui/kbd": "2.2.19",
    "@heroui/link": "2.2.21",
    "@heroui/modal": "^2.2.21",
    "@heroui/navbar": "2.2.22",
    "@heroui/progress": "^2.2.20",
    "@heroui/select": "^2.4.25",
    "@heroui/slider": "2.4.20",
    "@heroui/snippet": "2.2.25",
    "@heroui/spinner": "^2.2.21",
    "@heroui/switch": "2.2.22",
    "@heroui/system": "2.4.20",
    "@heroui/tabs": "^2.2.21",
    "@heroui/theme": "2.4.17",
    "@heroui/tooltip": "^2.2.21",
    "@heroui/use-theme": "2.1.10",
    "@react-aria/visually-hidden": "3.8.26",
    "@react-types/shared": "3.31.0",
    "clsx": "2.1.1",
    "concurrently": "^9.2.0",
    "framer-motion": "12.23.12",
    "nvm": "^0.0.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "6.30.1",
    "tailwind-variants": "1.0.0",
    "tailwindcss": "3.4.17"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.1.0",
    "@cypress/webpack-preprocessor": "^6.0.4",
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "24.0.13",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.1",
    "autoprefixer": "10.4.21",
    "cypress": "^14.5.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^54.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "madge": "^8.0.0",
    "postcss": "8.5.6",
    "prettier": "3.5.3",
    "sonar-scanner": "^3.1.0",
    "ts-loader": "^9.5.2",
    "typescript": "5.9.2",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^3.1.3",
    "webpack": "^5.100.2"
  }
}
